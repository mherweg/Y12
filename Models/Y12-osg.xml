<!-- instrument panel -->
	

<!-- crew -->
	<model>
		<name>pilot</name>
		<path>Aircraft/Y12/Models/pilot.xml</path>
		<nopreview/>
		<offsets> 
			<x-m>-7.4</x-m>
			<y-m>0.0</y-m>
			<z-m>0.83</z-m>
		</offsets>
	</model>

	<animation>
		<type>select</type>
		<object-name>pilot</object-name>
		<condition>
			<property>sim/model/Y12/crew/pilot/visible</property>
		</condition>
	</animation>

	<model>
		<name>walker-inside</name>
		<path>Aircraft/Y12/Models/walker-in.xml</path>
		<offsets> 
			<x-m>0.0</x-m>
			<y-m>0.0</y-m>
			<z-m>0.931</z-m>
		</offsets>
	</model>

	<animation>
		<type>select</type>
		<object-name>walker-inside</object-name>
		<condition>
			<and>
				<property>sim/model/Y12/crew/walker/visible</property>
				<greater-than>
					<property>sim/current-view/view-number</property>
					<value>0</value>
				</greater-than>
				<not>
					<property>sim/walker/outside</property>
				</not>
				<or>
					<equals>
						<property>sim/model/Y12/crew/cockpit-position</property>
						<value>1</value>
					</equals>
					<equals>
						<property>sim/model/Y12/crew/cockpit-position</property>
						<value>4</value>
					</equals>
				</or>
			</and>
		</condition>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>walker-inside</object-name>
		<property>orientation/groundslope-factored-pitch</property>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
