<!-- interior lighting lit -->
 <animation>
	<type>material</type>
	<object-name>hip_1</object-name>
	<property-base>sim/model/bluebird/lighting</property-base>
	<emission>
		<red-prop>waldo/emission/red</red-prop>
		<green-prop>waldo/emission/gb</green-prop>
		<blue-prop>waldo/emission/gb</blue-prop>
		<factor>0.6</factor>
	</emission>
	<specular>
		<factor-prop>interior-specular</factor-prop>
		<red>1</red>
		<green>1</green>
		<blue>1</blue>
	</specular>
	<ambient>
		<red-prop>waldo/emission/red</red-prop>
		<green-prop>waldo/emission/gb</green-prop>
		<blue-prop>waldo/emission/gb</blue-prop>
		<factor>0.4</factor>
		<offset>0.6</offset>
	</ambient>
	<diffuse>
		<factor-prop>interior-specular</factor-prop>
		<red>0.35</red>
		<green>0.35</green>
