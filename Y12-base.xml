	<help>
		<title>Harbin Y12</title>
		<key n="0">
			<name>equal=</name>
			<desc>open/close systems dialog</desc>
		</key>
		<key n="1">
			<name>tilde~</name>
			<desc>Shutdown/Startup Master Power</desc>
		</key>
		<key n="2">
			<name>backslash</name>
			<desc>Engage/Disengage Counter-grav</desc>
		</key>
		<key n="3">
			<name>space</name>
			<desc>Engage/Disengage Primary Engines</desc>
		</key>
		<key n="4">
			<name>[ ]</name>
			<desc>Increase/Decrease Maximum Speed</desc>
		</key>
		<key n="5">
			<name>Home</name>
			<desc>Counter-grav VTOL up</desc>
		</key>
		<key n="6">
			<name>End</name>
			<desc>Counter-grav VTOL down</desc>
		</key>
		<key n="7">
			<name>b</name>
			<desc>Reverse direction - fly backwards</desc>
		</key>
		<key n="8">
			<name>f / F</name>
			<desc>Select Next/Previous Door</desc>
		</key>
		<key n="9">
			<name>Ctrl-f</name>
			<desc>Open/Close Selected Door</desc>
		</key>
		<key n="10">
			<name>g / G</name>
			<desc>main Gear Up/Down</desc>
		</key>
		<key n="11">
			<name>Ctrl-g</name>
			<desc>Toggle Gear Deployment Mode</desc>
		</key>
		<key n="12">
			<name>Ctrl-w</name>
			<desc>Toggle Wheel Deployment Mode</desc>
		</key>
		<key n="13">
			<name>c / C</name>
			<desc>Next/Previous cabin position</desc>
		</key>
		<key n="14">
			<name>Alt-c</name>
			<desc>Jump to cockpit helm</desc>
		</key>
		<key n="15">
			<name>w / s</name>
			<desc>Walk Forward/Backward</desc>
		</key>
		<key n="16">
			<name>a / d</name>
			<desc>Walk: Slide sideways left/right</desc>
		</key>
		<key n="30">
			<name>Joystick</name>
			<desc>buttons:</desc>
		</key>
		<key n="31">
			<name>flaps button</name>
			<desc>Inc/Dec Max Speed power level</desc>
		</key>
		<key n="32">
			<name>elevator trim</name>
			<desc>Counter-grav VTOL up/down</desc>
		</key>
		<key n="33">
			<name>fire trigger</name>
			<desc>Reverse direction - fly backwards</desc>
		</key>
		<key n="34">
			<name>Middle Mouse Button</name>
			<desc>Pointer mode extras:</desc>
		</key>
		<key n="35">
			<name>Drag MMB up</name>
			<desc>Counter-grav VTOL up</desc>
		</key>
		<key n="36">
			<name>Drag MMB down</name>
			<desc>Counter-grav VTOL down</desc>
		</key>
		<key n="37">
			<name>click MMB</name>
			<desc>Center aileron and elevator</desc>
		</key>
	</help>

