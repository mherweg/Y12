<?xml version="1.0"?>
<!--== Y12 Explorer Hovercraft  version 8.8 ==-->
 
<PropertyList>
 <fx>

	<engine>
		<name>rumble</name>
		<mode>looped</mode>
		<path>Sounds/jet.wav</path>
		<property>velocities/airspeed-kt</property>
		<volume>
			<property>sim/model/Y12/sound/engines-volume-level</property>
		</volume>
		<pitch>
			<property>sim/model/Y12/sound/pitch-level</property>
		</pitch>
		<reference-dist>300</reference-dist>
		<max-dist>3000</max-dist>
	</engine>

	<engine>
		<name>overdrive</name>
		<mode>looped</mode>
		<path>Sounds/jet.wav</path>
		<condition>
			<greater-than-equals>
				<property>velocities/airspeed-kt</property>
				<value>6000</value>
			</greater-than-equals>
		</condition>
		<volume>
			<property>engines/engine/speed-max-powerlevel</property>
			<factor>0.3</factor>
			<offset>-1.9</offset>
		</volume>
		<pitch>
			<property>engines/engine/speed-max-powerlevel</property>
			<factor>0.25</factor>
			<offset>-0.75</offset>
		</pitch>
		<reference-dist>1000</reference-dist>
		<max-dist>6000</max-dist>
	</engine>

<!-- doors -->

	<door>
		<name>open-hatch0</name>
		<path>Aircraft/Y12/Sound/hatch-motor.wav</path>
		<condition>
			<and>
				<less-than>
					<property>sim/model/Y12/sound/door-direction</property>
					<value>0.5</value>
				</less-than>
				<and>
					<greater-than>
						<property>sim/model/Y12/doors/door[0]/position-norm</property>
						<value>0.115</value>
					</greater-than>
					<less-than>
						<property>sim/model/Y12/doors/door[0]/position-norm</property>
						<value>0.885</value>
					</less-than>
				</and>
			</and>
		</condition>
		<position>
			<x>-2.55</x> <!-- + = aft -->
			<y>-1.75</y> <!-- + = left -->
			<z>0</z>     <!-- + = up -->
		</position>
		<volume>
			<property>sim/model/Y12/sound/door0-volume</property>
			<factor>0.5</factor>
		</volume>
		<reference-dist>3</reference-dist>
		<max-dist>15</max-dist>
	</door>

